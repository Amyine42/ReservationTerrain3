@model List<Models.Utilisateur.UtilisateurListe>
<table>
    <tr>
        <td>
            Nom
        </td>
        <td>
            Prénom
        </td>
        <td>Email</td>
        <td>Tél</td>
        


    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.Nom</td>
            <td>@item.Prenom</td>
            <td>@item.Email</td>
            <td>@item.Tel</td>
            <td>
                <a asp-route-id="@item.Id" asp-action="Edit" asp-controller="Utilisateur">Editer</a>
              
            </td>

        </tr>



    }

</table>
<a asp-action="Create" asp-controller="Utilisateur">Ajouter</a>